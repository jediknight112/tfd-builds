{
  "name": "tfd-builds",
  "version": "1.0.0",
  "description": "The First Descendant Character Build Viewer",
  "main": "src/index.js",
  "scripts": {
    "dev": "npx vite",
    "dev:worker": "npm run build && wrangler dev",
    "build": "npx vite build",
    "preview": "npx vite preview",
    "deploy": "npm run build && wrangler deploy",
    "tailwind:build": "npx tailwindcss -i ./src/styles/input.css -o ./src/styles/output.css --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "npm run format:check"
  },
  "keywords": [
    "tfd",
    "builds",
    "gaming"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20260210.0",
    "@vitest/ui": "^4.0.18",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.49",
    "prettier": "^3.8.1",
    "tailwindcss": "^4.1.18",
    "vite": "^6.0.5",
    "vitest": "^4.0.18",
    "wrangler": "^4.64.0"
  },
  "dependencies": {
    "@cloudflare/kv-asset-handler": "^0.4.2",
    "axios": "^1.7.9",
    "lz-string": "^1.5.0"
  }
}
