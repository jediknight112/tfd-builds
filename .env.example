# TFD Build Viewer Configuration

# API Configuration
VITE_API_BASE_URL=https://tfd-cache.jediknight112.com
VITE_LANGUAGE_CODE=en

# Authentication Keys (Required for TFD Cache API)
# Get Worker API Key from your tfd-cache deployment
# Get Nexon API Key from https://openapi.nexon.com/
VITE_WORKER_API_KEY=your_worker_api_key_here
VITE_NEXON_API_KEY=your_nexon_api_key_here

# Feature Flags
VITE_ENABLE_BUILD_SHARING=false
VITE_ENABLE_ANALYTICS=false
